/////////////////////////////////////////////////////////////////////////////
//
// Menu
//
IDR_APPMENU MENU DISCARDABLE
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open",                 IDM_FILE_OPEN
        MENUITEM "Close",                 IDM_FILE_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "&Save",                 IDM_FILE_SAVE
        MENUITEM "&Save as",              IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                 IDM_EXIT
    END
    POPUP "&Settings"
    BEGIN
        MENUITEM "&Enable Shell Extension",     IDM_ESHELLEX
        MENUITEM "&Disable Shell Extension",    IDM_DSHELLEX
        MENUITEM SEPARATOR
        MENUITEM "Preferences",                 IDM_PREFER
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Help!",                 IDM_HELP
        MENUITEM SEPARATOR
        MENUITEM "&About...",              IDM_ABOUT
    END
END


